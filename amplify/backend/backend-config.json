{
  "api": {
    "caseexplorerui": {
      "service": "AppSync",
      "providerPlugin": "awscloudformation",
      "output": {
        "authConfig": {
          "defaultAuthentication": {
            "authenticationType": "API_KEY",
            "apiKeyConfig": {
              "apiKeyExpirationDays": 365,
              "description": "caselawexplorer-api"
            }
          },
          "additionalAuthenticationProviders": [
            {
              "authenticationType": "AMAZON_COGNITO_USER_POOLS",
              "userPoolConfig": {
                "userPoolId": "authcaseexploreruiauth"
              }
            }
          ]
        }
      }
    }
  },
  "auth": {
    "caseexploreruiauth": {
      "service": "Cognito",
      "providerPlugin": "awscloudformation",
      "dependsOn": [],
      "customAuth": false
    }
  },
  "hosting": {},
  "function": {
    "datafetcher": {
      "build": true,
      "providerPlugin": "awscloudformation",
      "service": "Lambda",
      "dependsOn": [
        {
          "category": "api",
          "resourceName": "caseexplorerui",
          "attributes": [
            "GraphQLAPIIdOutput"
          ]
        }
      ]
    },
    "queryhandler": {
      "build": true,
      "providerPlugin": "awscloudformation",
      "service": "Lambda",
      "dependsOn": [
        {
          "category": "api",
          "resourceName": "caseexplorerui",
          "attributes": [
            "GraphQLAPIIdOutput"
          ]
        }
      ]
    },
    "test": {
      "build": true,
      "providerPlugin": "awscloudformation",
      "service": "Lambda",
      "dependsOn": [
        {
          "category": "auth",
          "resourceName": "caseexploreruiauth",
          "attributes": [
            "UserPoolId"
          ]
        },
        {
          "category": "api",
          "resourceName": "caseexplorerui",
          "attributes": [
            "GraphQLAPIIdOutput"
          ]
        }
      ]
    },
    "networkstatistics": {
      "build": true,
      "providerPlugin": "awscloudformation",
      "service": "Lambda",
      "dependsOn": []
    },
    "batchdatafetcher": {
      "build": true,
      "providerPlugin": "awscloudformation",
      "service": "Lambda",
      "dependsOn": [
        {
          "category": "api",
          "resourceName": "caseexplorerui",
          "attributes": [
            "GraphQLAPIIdOutput"
          ]
        }
      ]
    },
    "caseexploreruidynamodbaccess": {
      "providerPlugin": "awscloudformation",
      "service": "LambdaLayer",
      "build": true
    },
    "caseexploreruiopensearchaccess": {
      "providerPlugin": "awscloudformation",
      "service": "LambdaLayer",
      "build": true
    },
    "caseexploreruiutils": {
      "providerPlugin": "awscloudformation",
      "service": "LambdaLayer",
      "build": true
    }
  },
  "storage": {}
}