{
  "api": {
    "caseexplorerui": {
      "service": "AppSync",
      "providerPlugin": "awscloudformation",
      "output": {
        "authConfig": {
          "defaultAuthentication": {
            "authenticationType": "API_KEY",
            "apiKeyConfig": {
              "apiKeyExpirationDays": 365,
              "description": "caselawexplorer-api"
            }
          },
          "additionalAuthenticationProviders": []
        }
      }
    }
  },
  "auth": {
    "caseexploreruiauth": {
      "service": "Cognito",
      "providerPlugin": "awscloudformation",
      "dependsOn": [],
      "customAuth": false
    }
  },
  "hosting": {},
  "function": {
    "caseexploreruidataaccesslayer": {
      "providerPlugin": "awscloudformation",
      "service": "LambdaLayer",
      "build": true
    },
    "caseexploreruiglobalsettingslayer": {
      "providerPlugin": "awscloudformation",
      "service": "LambdaLayer",
      "build": true
    },
    "datafetcher": {
      "build": true,
      "providerPlugin": "awscloudformation",
      "service": "Lambda",
      "dependsOn": [
        {
          "category": "api",
          "resourceName": "caseexplorerui",
          "attributes": [
            "GraphQLAPIIdOutput",
            "GraphQLAPIEndpointOutput",
            "GraphQLAPIKeyOutput"
          ]
        }
      ]
    },
    "queryhandler": {
      "build": true,
      "providerPlugin": "awscloudformation",
      "service": "Lambda",
      "dependsOn": [
        {
          "category": "api",
          "resourceName": "caseexplorerui",
          "attributes": [
            "GraphQLAPIIdOutput",
            "GraphQLAPIEndpointOutput",
            "GraphQLAPIKeyOutput"
          ]
        },
        {
          "category": "auth",
          "resourceName": "caseexploreruiauth",
          "attributes": [
            "UserPoolId"
          ]
        }
      ]
    },
    "test": {
      "build": true,
      "providerPlugin": "awscloudformation",
      "service": "Lambda",
      "dependsOn": [
        {
          "category": "api",
          "resourceName": "caseexplorerui",
          "attributes": [
            "GraphQLAPIIdOutput",
            "GraphQLAPIEndpointOutput",
            "GraphQLAPIKeyOutput"
          ]
        }
      ]
    },
    "networkstatistics": {
      "build": true,
      "providerPlugin": "awscloudformation",
      "service": "Lambda",
      "dependsOn": [
        {
          "category": "api",
          "resourceName": "caseexplorerui",
          "attributes": [
            "GraphQLAPIIdOutput"
          ]
        }
      ]
    }
  },
  "storage": {}
}